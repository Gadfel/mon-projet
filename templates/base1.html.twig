                <ul class="navbar-nav  col-6 col-md-4 col-lg-10 col-xl-10">
                  <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="{{ path('home') }}">Accueil</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="{{ path('machine_index') }}">Machines</a>
                  </li>
                  <li class="nav-item dropdown">
                    <a class="nav-link active" aria-current="page" href="{{ path('category_index') }}" id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">Produits</a>
                      
                   
                    <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                      <li><a class="dropdown-item" href="{{ path('category_index') }}">Café</a></li>
                      <li><a class="dropdown-item" href="{{ path('category_index') }}">Thé</a></li>
                      <li><a class="dropdown-item" href="{{ path('category_index') }}">Chocolat</a></li>
                      <li><a class="dropdown-item" href="{{ path('category_index') }}">Gourmant</a></li>
                      <li><a class="dropdown-item" href="{{ path('category_index') }}">Potage</a></li>
                      <li><a class="dropdown-item" href="{{ path('category_index') }}">Potage</a></li>
                    </ul>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="{{ path('contact_index') }}">Contact</a>
                  </li>
                  <li class="nav-item dropdown">
                    {% if is_granted ('IS_AUTHENTICATED_FULLY') %} 
                    <a class="nav-link active" aria-current="page" href="{{ path('user_index') }}" id="navbarDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">Profil</a>
                      
                   
                    <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                      <li><a class="dropdown-item" href="{{ path('user_index') }}" > Mon Profil</a></li>
                      <li><a class="dropdown-item" href="{{ path('app_logout') }}">Déconnexion </a></li>
                    </ul>
                    {% else %} 
                  </li>
                      <li class="nav-item">
                        <a class="nav-link" href="{{ path('app_login') }}">Connexion</a>
                      </li>
                     {% endif %} 
                    </li>
                  <li class="nav-item">
                    <a href="{{ path('cart_index') }}"><img class="panier1" src="{{ asset('images/panier1.png')}}" alt="panier"></a>
                  </li>
                </ul>
              </div>
            </div>
          </nav>
    </header>

    <main>

      {% include "partage/cookie.html.twig" %}    
        {% block body %}{% endblock %}
    </main>
    

    <footer class=" container-fluid1 footer">
        <div class="row text-center ">
            <div class="col-4" id="apropos">
                <h5 class="h5">Apropos</h5>
                <div class="dropdown-divider"></div>
                <p><a href="{{ path('contact_index') }}">données personnelles</a></p>
                <p><a href="{{ path('app_mentions_legales')}}">Mentions Légales</a></p>
                <p><a href="{{ path('app_cgu')}}">CGU</a> , 
                   <a href="{{ path('app_cgv')}}">CGV</a></p>
                <p><a href="{{ path('politique') }}">Politique de Confidentialité</a></p>
                <p><a href="{{ path('contact_index') }}">Contact</a></p>
                {% if is_granted('ROLE_ADMIN') %}
                    <p><a href="{{ path('admin') }}">Admin</a></p>
                {% endif %}
            </div>
            <div class="col-4 my-auto">
               <a href="https://etsgadfel.fr/"> <img src="{{ asset('images/logo.png')}}" alt="logo A.G " class="w-100" target="_blank"></a>
            </div>
